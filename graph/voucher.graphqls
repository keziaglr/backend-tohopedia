type Voucher{
    id: Int!
    name: String!
    description: String!
    discountRate: Int!
    code: String!
    tnc: String!
    startTime: Time!
    endTime: Time!
}

extend type Query{
    getVoucherByProduct(productId: Int!): [Voucher]
    vouchers: [Voucher!]!
    getVoucherById(voucherId: Int!): Voucher!
    getVoucherCart(userId: Int!): [Voucher!]!
}

extend type Mutation{
    createUserVoucher(voucherId: Int!, userId: Int!): UserVoucher!
}